{
    "version": 2,
    "buildCommand": "npm run vercel-build",
    "outputDirectory": "dist/public",
    "framework": null,
    "functions": {
        "api/index.ts": {
            "runtime": "@vercel/node@3.0.0"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.ts"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}